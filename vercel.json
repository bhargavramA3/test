{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "builds": [
    { "src": "*.php", "use": "vercel-php@0.6.0" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "*.jpg", "use": "@vercel/static" }
  ],
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" }
    ]
  }